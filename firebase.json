  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    /*"redirects": [
      {
        "source": "/",
        "destination": "*//**",
        "type": 301
      }
    ],*/
    "rewrites": [
     /* {
        "source": "/app*//**",
        "destination": "/index.html"
      },*/
      {
        "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "function": "app"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
